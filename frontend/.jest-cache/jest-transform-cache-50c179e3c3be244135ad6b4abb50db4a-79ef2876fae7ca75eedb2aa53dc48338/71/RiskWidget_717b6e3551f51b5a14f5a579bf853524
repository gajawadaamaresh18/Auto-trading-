      /* istanbul ignore next */
      cov_217fmlsbxs().b[21][0]++;
      cov_217fmlsbxs().s[52]++;
      stopLossPrice = currentPrice * (1 - riskSettings.stopLoss.value / 100);
    } else {
      /* istanbul ignore next */
      cov_217fmlsbxs().b[21][1]++;
      cov_217fmlsbxs().s[53]++;
      stopLossPrice = currentPrice - riskSettings.stopLoss.value;
    }
