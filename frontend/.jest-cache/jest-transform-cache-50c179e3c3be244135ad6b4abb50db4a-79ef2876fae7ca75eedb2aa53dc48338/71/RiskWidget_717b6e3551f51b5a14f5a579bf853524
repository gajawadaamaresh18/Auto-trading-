const getRiskColor = riskTolerance => {
  /* istanbul ignore next */
  cov_217fmlsbxs().f[45]++;
  cov_217fmlsbxs().s[142]++;
  switch (riskTolerance) {
    case 'low':
      /* istanbul ignore next */
      cov_217fmlsbxs().b[71][0]++;
      cov_217fmlsbxs().s[143]++;
      return theme_1.default.colors.success[100];
    case 'medium':
      /* istanbul ignore next */
      cov_217fmlsbxs().b[71][1]++;
      cov_217fmlsbxs().s[144]++;
      return theme_1.default.colors.warning[100];
    case 'high':
      /* istanbul ignore next */
      cov_217fmlsbxs().b[71][2]++;
      cov_217fmlsbxs().s[145]++;
      return theme_1.default.colors.error[100];
    default:
      /* istanbul ignore next */
      cov_217fmlsbxs().b[71][3]++;
      cov_217fmlsbxs().s[146]++;
      return theme_1.default.colors.neutral[100];
  }
};
