      /* istanbul ignore next */
      cov_217fmlsbxs().b[23][0]++;
      cov_217fmlsbxs().s[57]++;
      takeProfitPrice = currentPrice * (1 + riskSettings.takeProfit.value / 100);
    } else {
      /* istanbul ignore next */
      cov_217fmlsbxs().b[23][1]++;
      cov_217fmlsbxs().s[58]++;
