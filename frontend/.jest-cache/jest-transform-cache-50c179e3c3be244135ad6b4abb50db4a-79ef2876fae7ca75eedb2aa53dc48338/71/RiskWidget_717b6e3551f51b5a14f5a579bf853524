  /* istanbul ignore next */
  (cov_217fmlsbxs().s[59]++, 0);
  let maxPositionSize =
  /* istanbul ignore next */
  (cov_217fmlsbxs().s[60]++, 0);
  /* istanbul ignore next */
  cov_217fmlsbxs().s[61]++;
