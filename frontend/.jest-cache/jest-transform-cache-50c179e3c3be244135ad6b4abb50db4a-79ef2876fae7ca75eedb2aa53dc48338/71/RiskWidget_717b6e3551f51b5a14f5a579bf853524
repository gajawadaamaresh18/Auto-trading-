  /* istanbul ignore next */
  cov_217fmlsbxs().f[10]++;
  const warnings =
  /* istanbul ignore next */
  (cov_217fmlsbxs().s[47]++, []);
  let isValid =
  /* istanbul ignore next */
  (cov_217fmlsbxs().s[48]++, true);
  // Calculate stop loss price
  let stopLossPrice =
  /* istanbul ignore next */
  (cov_217fmlsbxs().s[49]++, 0);
  /* istanbul ignore next */
  cov_217fmlsbxs().s[50]++;
