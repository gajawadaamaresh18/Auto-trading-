      /* istanbul ignore next */
      cov_217fmlsbxs().b[24][2]++;
      const riskAmount =
      /* istanbul ignore next */
      (cov_217fmlsbxs().s[68]++, portfolioValue * riskSettings.positionSizing.maxRiskPerTrade / 100);
      const riskPerShare =
      /* istanbul ignore next */
      (cov_217fmlsbxs().s[69]++, currentPrice - stopLossPrice);
      /* istanbul ignore next */
      cov_217fmlsbxs().s[70]++;
