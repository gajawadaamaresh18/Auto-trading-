  /* istanbul ignore next */
  {
    cov_217fmlsbxs().b[20][1]++;
  }
  // Calculate take profit price
  let takeProfitPrice =
  /* istanbul ignore next */
  (cov_217fmlsbxs().s[54]++, 0);
  /* istanbul ignore next */
  cov_217fmlsbxs().s[55]++;
