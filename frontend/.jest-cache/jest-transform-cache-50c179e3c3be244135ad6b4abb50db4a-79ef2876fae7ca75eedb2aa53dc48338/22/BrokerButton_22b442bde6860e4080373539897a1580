              /* istanbul ignore next */
              (cov_1v0pgeskyo().b[25][1]++, 'Connect')
            })]
          }))
        })
      })]
    });
  };
  /* istanbul ignore next */
  cov_1v0pgeskyo().s[57]++;
  if (
  /* istanbul ignore next */
  (cov_1v0pgeskyo().b[27][0]++, onPress) &&
  /* istanbul ignore next */
  (cov_1v0pgeskyo().b[27][1]++, variant === 'card')) {
    /* istanbul ignore next */
    cov_1v0pgeskyo().b[26][0]++;
    cov_1v0pgeskyo().s[58]++;
    return (0, jsx_runtime_1.jsx)(react_native_1.TouchableOpacity, {
      onPress: onPress,
      activeOpacity: 0.8,
      style: styles.touchableContainer,
      children: (0, jsx_runtime_1.jsx)(CardContent, {})
    });
  } else
  /* istanbul ignore next */
  {
    cov_1v0pgeskyo().b[26][1]++;
  }
  cov_1v0pgeskyo().s[59]++;
  return (0, jsx_runtime_1.jsx)(CardContent, {});
};
