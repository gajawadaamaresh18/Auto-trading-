function _interopRequireWildcard(e, t) { if ("function" == typeof WeakMap) var r = new WeakMap(), n = new WeakMap(); return (_interopRequireWildcard = function _interopRequireWildcard(e, t) { if (!t && e && e.__esModule) return e; var o, i, f = { __proto__: null, default: e }; if (null === e || "object" != typeof e && "function" != typeof e) return f; if (o = t ? n : r) { if (o.has(e)) return o.get(e); o.set(e, f); } for (var _t in e) "default" !== _t && {}.hasOwnProperty.call(e, _t) && ((i = (o = Object.defineProperty) && Object.getOwnPropertyDescriptor(e, _t)) && (i.get || i.set) ? o(f, _t, i) : f[_t] = e[_t]); return f; })(e, t); }
var calculateRisk = exports.calculateRisk = function calculateRisk(riskSettings, currentPrice, portfolioValue) {
  var warnings = [];
  var isValid = true;
  var stopLossPrice = 0;
  if (riskSettings.stopLoss.enabled) {
    if (riskSettings.stopLoss.type === 'percentage') {
      stopLossPrice = currentPrice * (1 - riskSettings.stopLoss.value / 100);
    } else {
      stopLossPrice = currentPrice - riskSettings.stopLoss.value;
    }
  }
  var takeProfitPrice = 0;
  if (riskSettings.takeProfit.enabled) {
    if (riskSettings.takeProfit.type === 'percentage') {
      takeProfitPrice = currentPrice * (1 + riskSettings.takeProfit.value / 100);
    } else {
      takeProfitPrice = currentPrice + riskSettings.takeProfit.value;
    }
  }
  var positionSize = 0;
  var maxPositionSize = 0;
  switch (riskSettings.positionSizing.method) {
    case 'fixed':
      positionSize = riskSettings.positionSizing.value;
      maxPositionSize = positionSize;
      break;
    case 'percentage':
      positionSize = portfolioValue * riskSettings.positionSizing.value / 100 / currentPrice;
      maxPositionSize = positionSize;
      break;
    case 'risk_based':
      var _riskAmount = portfolioValue * riskSettings.positionSizing.maxRiskPerTrade / 100;
      var riskPerShare = currentPrice - stopLossPrice;
      if (riskPerShare > 0) {
        positionSize = _riskAmount / riskPerShare;
        maxPositionSize = positionSize;
      }
      break;
  }
  var riskAmount = positionSize * (currentPrice - stopLossPrice);
  var rewardAmount = positionSize * (takeProfitPrice - currentPrice);
  var riskRewardRatio = riskAmount > 0 ? rewardAmount / riskAmount : 0;
  var portfolioRiskPercent = riskAmount / portfolioValue * 100;
  if (portfolioValue <= 0) {
    warnings.push('Portfolio value is zero');
    isValid = false;
  }
  if (currentPrice <= 0) {
    warnings.push('Invalid price');
    isValid = false;
  }
  if (portfolioRiskPercent > riskSettings.riskProfile.maxPortfolioRisk) {
    warnings.push(`Portfolio risk (${portfolioRiskPercent.toFixed(2)}%) exceeds maximum (${riskSettings.riskProfile.maxPortfolioRisk}%)`);
    isValid = false;
  }
  if (riskRewardRatio < 1 && riskSettings.takeProfit.enabled) {
    warnings.push(`Risk/Reward ratio (${riskRewardRatio.toFixed(2)}) is below 1:1`);
  }
  if (stopLossPrice <= 0) {
    warnings.push('Stop loss price must be positive');
    isValid = false;
  }
  if (takeProfitPrice <= currentPrice && riskSettings.takeProfit.enabled) {
    warnings.push('Take profit price must be above entry price');
    isValid = false;
  }
  return {
    entryPrice: currentPrice,
    stopLossPrice: stopLossPrice,
    takeProfitPrice: takeProfitPrice,
    positionSize: Math.floor(positionSize),
    riskAmount: riskAmount,
    rewardAmount: rewardAmount,
    riskRewardRatio: riskRewardRatio,
    portfolioRiskPercent: portfolioRiskPercent,
    maxPositionSize: Math.floor(maxPositionSize),
    warnings: warnings,
    isValid: isValid
  };
};
var RISK_PROFILES = exports.RISK_PROFILES = {
  conservative: {
    maxPortfolioRisk: 1.0,
    maxDailyLoss: 2.0,
    maxConcurrentTrades: 3,
    riskTolerance: 'low'
  },
  moderate: {
    maxPortfolioRisk: 2.0,
    maxDailyLoss: 4.0,
    maxConcurrentTrades: 5,
    riskTolerance: 'medium'
  },
  aggressive: {
    maxPortfolioRisk: 5.0,
    maxDailyLoss: 10.0,
    maxConcurrentTrades: 10,
    riskTolerance: 'high'
  }
};
