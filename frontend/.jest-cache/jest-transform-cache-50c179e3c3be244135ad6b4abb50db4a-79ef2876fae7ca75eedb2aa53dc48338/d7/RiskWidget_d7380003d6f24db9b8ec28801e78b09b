var getRiskColor = function getRiskColor(riskTolerance) {
  switch (riskTolerance) {
    case 'low':
      return _theme.default.colors.success[100];
    case 'medium':
      return _theme.default.colors.warning[100];
    case 'high':
      return _theme.default.colors.error[100];
    default:
      return _theme.default.colors.neutral[100];
  }
};
