      var stopLossToggle = getByTestId('stop-loss-toggle');
      (0, _reactNative.fireEvent)(stopLossToggle, 'onValueChange', false);
      expect(defaultProps.onSettingsChange).toHaveBeenCalledWith({
        stopLoss: Object.assign({}, mockRiskSettings.stopLoss, {
          enabled: false
        })
      });
    });
    it('changes stop loss type from percentage to fixed', function () {
      var _render0 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps))
        })),
        getByTestId = _render0.getByTestId;
      var typeSelector = getByTestId('stop-loss-type-selector');
      _reactNative.fireEvent.press(typeSelector);
      var fixedOption = getByTestId('stop-loss-type-fixed');
      _reactNative.fireEvent.press(fixedOption);
      expect(defaultProps.onSettingsChange).toHaveBeenCalledWith({
        stopLoss: Object.assign({}, mockRiskSettings.stopLoss, {
          type: 'fixed'
        })
      });
    });
    it('enables trailing stop when toggle is pressed', function () {
      var _render1 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps))
