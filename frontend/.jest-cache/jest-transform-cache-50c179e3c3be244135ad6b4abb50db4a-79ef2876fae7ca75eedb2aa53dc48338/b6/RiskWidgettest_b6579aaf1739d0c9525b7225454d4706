      expect(getByText(/Stop loss is required/)).toBeTruthy();
    });
  });
  describe('User Interactions', function () {
    it('updates stop loss value when user changes input', function () {
      var _render8 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps))
        })),
        getByDisplayValue = _render8.getByDisplayValue,
        getByTestId = _render8.getByTestId;
      var stopLossInput = getByDisplayValue('2');
      _reactNative.fireEvent.changeText(stopLossInput, '3');
      expect(defaultProps.onSettingsChange).toHaveBeenCalledWith({
        stopLoss: Object.assign({}, mockRiskSettings.stopLoss, {
          value: 3
        })
      });
    });
    it('toggles stop loss enabled state', function () {
      var _render9 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps))
