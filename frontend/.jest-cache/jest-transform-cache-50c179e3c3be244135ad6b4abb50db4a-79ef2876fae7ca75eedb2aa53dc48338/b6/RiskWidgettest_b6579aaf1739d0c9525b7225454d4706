var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _native = require("styled-components/native");
var _RiskWidget = require("../../components/RiskWidget/RiskWidget");
var _theme = _interopRequireDefault(require("../../theme"));
var _jsxRuntime = require("react/jsx-runtime");
var mockRiskSettings = {
  id: 'risk-1',
  formulaId: 'formula-1',
  userId: 'user-1',
  stopLoss: {
    enabled: true,
    type: 'percentage',
    value: 2.0,
    trailing: false,
    trailingStep: 0.5
  },
  takeProfit: {
    enabled: true,
    type: 'percentage',
    value: 4.0
  },
  positionSizing: {
    method: 'risk_based',
    value: 1000,
    maxRiskPerTrade: 1.0
  },
  riskProfile: {
    maxPortfolioRisk: 5.0,
    maxDailyLoss: 2.0,
    maxConcurrentTrades: 5,
    riskTolerance: 'medium'
  },
  createdAt: '2023-01-01T00:00:00Z',
  updatedAt: '2023-01-01T00:00:00Z',
  isActive: true
};
var TestWrapper = function TestWrapper(_ref) {
  var children = _ref.children;
  return (0, _jsxRuntime.jsx)(_native.ThemeProvider, {
    theme: _theme.default,
    children: children
  });
};
describe('RiskWidget', function () {
  var defaultProps = {
    riskSettings: mockRiskSettings,
    currentPrice: 100,
    portfolioValue: 100000,
    onSettingsChange: jest.fn(),
    onSaveSettings: jest.fn(),
    readOnly: false
  };
  beforeEach(function () {
    jest.clearAllMocks();
  });
  describe('Risk Calculations', function () {
    it('calculates stop loss price correctly for percentage', function () {
      var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps))
        })),
        getByText = _render.getByText;
      expect(getByText('$98.00')).toBeTruthy();
    });
    it('calculates take profit price correctly for percentage', function () {
      var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps))
        })),
        getByText = _render2.getByText;
      expect(getByText('$104.00')).toBeTruthy();
    });
    it('calculates position size based on risk', function () {
      var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps))
        })),
        getByText = _render3.getByText;
      expect(getByText(/Position Size/)).toBeTruthy();
    });
    it('calculates risk/reward ratio correctly', function () {
      var _render4 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps))
        })),
        getByText = _render4.getByText;
      expect(getByText('2.00')).toBeTruthy();
    });
  });
  describe('Risk Warnings', function () {
    it('shows warning when portfolio risk exceeds limit', function () {
      var highRiskSettings = Object.assign({}, mockRiskSettings, {
        riskProfile: Object.assign({}, mockRiskSettings.riskProfile, {
          maxPortfolioRisk: 1.0
        })
      });
      var _render5 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps, {
            riskSettings: highRiskSettings
          }))
        })),
        getByText = _render5.getByText;
      expect(getByText(/Portfolio risk exceeds maximum/)).toBeTruthy();
    });
    it('shows warning when risk/reward ratio is below 1:1', function () {
      var poorRiskSettings = Object.assign({}, mockRiskSettings, {
        takeProfit: Object.assign({}, mockRiskSettings.takeProfit, {
          value: 1.0
        })
      });
      var _render6 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
          children: (0, _jsxRuntime.jsx)(_RiskWidget.RiskWidget, Object.assign({}, defaultProps, {
            riskSettings: poorRiskSettings
          }))
        })),
        getByText = _render6.getByText;
      expect(getByText(/Risk\/Reward ratio is below 1:1/)).toBeTruthy();
    });
    it('shows warning when stop loss is not set', function () {
      var noStopLossSettings = Object.assign({}, mockRiskSettings, {
        stopLoss: Object.assign({}, mockRiskSettings.stopLoss, {
          enabled: false
        })
      });
      var _render7 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestWrapper, {
